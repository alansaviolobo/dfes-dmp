{
  "name": "Default map layer configuration defined by amche-goa community",
  "areaOfInterest": "Goa, India",
  "version": "0.1.0",
  "url": "https://github.com/publicmap/amche-goa/blob/main/config/index.json",
  "map": {
    "center": [
      73.8,
